{
  "name": "@double-great/remark-lint-link-text",
  "version": "0.8.0",
  "description": "A remark-lint plugin that warns against non-descriptive link text.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pretest": "npm run lint",
    "prepare": "husky install",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "pretty-quick": "pretty-quick",
    "write-docs": "tsc && node dist/docs.js"
  },
  "author": "@double-great",
  "license": "MIT",
  "dependencies": {
    "tinyurl": "^1.1.7",
    "unified-lint-rule": "^2.1.1",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/dedent": "^0.7.0",
    "@types/github-slugger": "^1.3.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "dedent": "^0.7.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.4.6",
    "eslint-plugin-node": "^11.1.0",
    "github-slugger": "^1.4.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "remark": "^14.0.2",
    "typescript": "^4.7.2"
  },
  "keywords": [
    "remark",
    "remark-plugin",
    "remark-lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/double-great/remark-lint-link-text.git"
  },
  "bugs": {
    "url": "https://github.com/double-great/remark-lint-link-text/issues"
  },
  "homepage": "https://github.com/double-great/remark-lint-link-text#readme",
  "jest": {
    "clearMocks": true,
    "transformIgnorePatterns": [
      "!node_modules/"
    ],
    "moduleNameMapper": {
      "^..?/rule.js$": "<rootDir>/src/rule.ts",
      "^..?/docs.js$": "<rootDir>/src/docs.ts",
      "^..?/utils.js$": "<rootDir>/src/rules/utils.ts",
      "^..?/rules/(.*).js$": "<rootDir>/src/rules/$1.ts"
    }
  }
}
